{
	"name": "WikimediaApiPortal",
	"version": "1.0",
	"license-name": "GPL-3.0-only",
	"author": [
		"Dejan Savuljesku",
		"Robert Vogel"
	],
	"type": "skin",
	"url": "https://www.mediawiki.org/wiki/Skin:WikimediaApiPortal",
	"descriptionmsg": "wikimediaapiportal-skin-desc",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"ValidSkinNames": {
		"wikimediaapiportal": {
			"DisplayName": "WikimediaApiPortal",
			"class": "MediaWiki\\Skin\\WikimediaApiPortal\\Skin"
		}
	},
	"MessagesDirs": {
		"WikimediaApiPortal": "i18n"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Skin\\WikimediaApiPortal\\": "src/"
	},
	"TestAutoloadNamespaces": {
		"MediaWiki\\Skin\\WikimediaApiPortal\\Tests\\": "tests/phpunit/"
	},
	"AvailableRights": [
		"wikimediaapiportal-see-page-actions"
	],
	"GroupPermissions": {
		"user": {
			"wikimediaapiportal-see-page-actions": true
		}
	},
	"ResourceModules": {
		"skin.wikimediaapiportal.styles": {
			"styles": {
				"resources/stylesheets/xl.less": { "media": "all and ( min-width: 1301px )" },
				"resources/stylesheets/lg.less": { "media": "all and ( min-width: 992px ) and ( max-width: 1300px )" },
				"resources/stylesheets/sm.less": { "media": "all and ( max-width: 991px )" },
				"resources/stylesheets/print.less": { "media": "print" },
				"dist/bootstrap.css": {}
			}
		},
		"skin.wikimediaapiportal": {
			"scripts": [
				"dist/bootstrap.bundle.js",
				"resources/script/searchform.js",
				"resources/script/scrollAdjust.js"
			]
		},
		"skin.wikimediaapiportal.mainpage": {
			"styles": {
				"resources/stylesheets/mainpage/xl.less": { "media": "all and ( min-width: 1301px )" },
				"resources/stylesheets/mainpage/lg.less": { "media": "all and ( min-width: 992px ) and ( max-width: 1300px )" },
				"resources/stylesheets/mainpage/sm.less": { "media": "all and ( max-width: 991px )" }
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "WikimediaApiPortal"
	},
	"Hooks": {
		"AuthChangeFormFields": "MediaWiki\\Skin\\WikimediaApiPortal\\Hook\\AuthChangeFormFields\\RemoveRealName::callback"
	},
	"manifest_version": 2
}
