{{{html-headelement}}}
<div class="flex-fill wm-container container">
	<div class="wm-header fixed-top d-none d-md-none d-lg-flex d-xl-flex row">
		<div class="wm-header-inner col">
			<a class="mw-jump-link" href="#content">{{jumptocontent}}</a>
			<div class="row">
				<div class="flex-grow-0 col">
					{{{Logo}}}
				</div>
				<div class="col">
					{{{NavBar}}}
				</div>
				<div class="ml-auto col-12 col-lg col">
					<div class="flex-row-reverse row">
						{{{UserMenu}}}
						{{{NotificationAlert}}}
						<div class="wm-search-container d-none d-md-none d-sm-none d-lg-none d-xl-flex">
							{{{SearchField}}}
						</div>
						<div class="wm-search-container search-lg d-none d-sm-none d-xl-none d-md-flex d-lg-flex">
							{{{SearchButton}}}
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="wm-header fixed-top d-block d-sm-block d-md-block d-lg-none d-xl-none row">
		<div class="wm-header-inner col">
			<a class="mw-jump-link" href="#content">{{jumptocontent}}</a>
			<div class="row">
				<div class="wm-nav col">
					{{{NavMenu}}}
				</div>
				<div class="col">
					{{{Logo}}}
				</div>
				<div class="wm-tools col">
					<div class="flex-row-reverse row">
						{{{UserMenu}}}
						{{{NotificationAlert}}}
						<div class="wm-search-container search-sm">
							{{{SearchButton}}}
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="wm-content row">
		{{#if html-siteNotice}}
			<div id="siteNotice" class="mw-body-content w-100">{{{html-siteNotice}}}</div>
		{{/if}}
		<div class="page-nav-cnt d-none d-lg-block d-xl-block col-2 no-gutters">
			{{#SecondaryNav}}
				{{{SecondaryNav}}}
			{{/SecondaryNav}}
		</div>
		<div class="wm-main-content col-10">
			{{{Content}}}
		</div>
	</div>
</div>
{{{Footer}}}
